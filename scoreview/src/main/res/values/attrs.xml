<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScoreView">
        <attr name="outer_circle_stroke" format="dimension"/>
        <attr name="inner_circle_stroke" format="dimension"/>
        <attr name="circles_padding" format="dimension"/>
        <attr name="score_text_size" format="dimension"/>
        <attr name="text_spacing" format="dimension"/>
        <attr name="upper_and_lower_text_size" format="dimension"/>

        <attr name="outer_circle_color" format="color"/>
        <attr name="start_color" format="color"/>
        <attr name="middle_color" format="color"/>
        <attr name="end_color" format="color"/>
        <attr name="upper_and_lower_text_color" format="color"/>

        <attr name="upper_text" format="string"/>
        <attr name="lower_text" format="string"/>
    </declare-styleable>
</resources>

